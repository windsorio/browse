{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
    { "open": "'", "close": "'", "notIn": ["string", "comment"] },
    { "open": "`", "close": "`", "notIn": ["string", "comment"] },
    { "open": "|", "close": "|", "notIn": ["string", "comment"] }
  ],
  "autoCloseBefore": ";=}]) \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["|", "|"]
  ],
  "markers": {
    "start": "^\\s*#\\s*#?region\\b",
    "end": "^\\s*#\\s*#?endregion\\b"
  }
}
